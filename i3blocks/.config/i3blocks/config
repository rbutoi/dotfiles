command=/usr/share/i3blocks/$BLOCK_NAME
separator_block_width=15
markup=none

[window_title]
command=xprop -id $(xdotool getactivewindow) | grep WM_NAME | cut -d'"' -f2
interval=1

[load_average]
interval=5

[volume]
instance=Master
interval=1
signal=10

[time]
command=date +'%a %b %d %H:%M'
interval=5
